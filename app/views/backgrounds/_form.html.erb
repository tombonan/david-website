<div class = "blog-form-container">
<%= form_for @background do |b| %>
    <p>
    <%= b.label :About %><br />
    <%= b.text_area :about, :class => 'ckeditor'  %>
  </p>

  <p>
    <%= b.label :Contact %><br />
    <%= b.text_area :contact, :class => 'ckeditor'  %>
  </p>

  <p><%= b.submit "Update"%></p>

<% end %>
</div>